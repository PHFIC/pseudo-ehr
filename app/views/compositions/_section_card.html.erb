<div class='card bg-dark text-white'>
  <div class='card-header' id="sectionHeader-<%= index %>" data-bs-toggle="tooltip" title="<%= strip_tags(section["text"]) %>">
    <button class="btn btn-link" type="button" data-bs-toggle="collapse" data-bs-target="#sectionBody-<%= index %>" aria-expanded="false" aria-controls="sectionHeader-<%= index %>">
        <%= section["title"] %>
    </button>
  </div>
  <div id="sectionBody-<%= index %>" class="collapse" aria-labelledby="sectionHeader-<%= index %>" data-bs-parent="#sections">
    <div class='card-body'>
      <br>
      <% section["objects"].each do |object| %>
        <%= render object.resourceType + "_card", object: object %>
        <br>
      <% end %>
    </div>
  </div>
</div>

<!DOCTYPE html>
<html>
  <%= render 'partials/head' %>
  <body>
    <%= render 'partials/header' %>
    <%= render 'partials/messages' %>
    <div id='content'>
      <main role='main'>
        <div class="body-padding center flex-column" style="margin: 1% auto;">
          <%= render 'partials/queries' %>
          <%= yield %>
        </div>
      </main>
      <%= render 'partials/footer' %>
    </div>
    <%= yield :scripts %>
  </body>
</html>
